  "bugs": {
    "url": "https://github.com/ibhuiyan17/spotify-ppm/issues"
  },
  "homepage": "https://github.com/ibhuiyan17/spotify-ppm#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "cookie-parser": "1.3.2",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "nodemon": "^1.19.1",
    "querystring": "~0.2.0",
    "request": "~2.83.0",
    "simple-statistics": "^7.0.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
