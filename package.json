  "description": "Allows users to generate new playlists out of seeds including tracks, artists, and genres.",
  "engines": {
    "node": "12.x"
  },
  "main": "./server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
